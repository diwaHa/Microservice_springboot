spring:
  datasource:
    url: jdbc:mysql://localhost:3306/microv
    username: root
    password: root
  jpa:
    hibernate:
      ddl-auto: update
    properties:
      hibernate:
        dialect: org.hibernate.dialect.MySQL8Dialect
        format_sql: false
    show-sql: true
    open-in-view: false # Add this line to disable the open-in-view pattern

server:
  port: 9001